<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Brush.WindowBackground" Color="{x:Static SystemColors.WindowColor}" />
    <SolidColorBrush x:Key="Brush.NavigationBackground" Color="{x:Static SystemColors.WindowFrameColor}" />
    <SolidColorBrush x:Key="Brush.NavigationBorder" Color="{x:Static SystemColors.WindowFrameColor}" />
    <SolidColorBrush x:Key="Brush.NavigationTitle" Color="{x:Static SystemColors.WindowTextColor}" />
    <SolidColorBrush x:Key="Brush.NavigationSubtitle" Color="{x:Static SystemColors.GrayTextColor}" />
    <SolidColorBrush x:Key="Brush.NavigationItemSelectedBackground" Color="{x:Static SystemColors.HighlightColor}" />
    <SolidColorBrush x:Key="Brush.NavigationItemHoverBackground" Color="{x:Static SystemColors.HotTrackColor}" />
    <SolidColorBrush x:Key="Brush.ContentBackground" Color="{x:Static SystemColors.WindowColor}" />
    <SolidColorBrush x:Key="Brush.SurfaceBackground" Color="{x:Static SystemColors.WindowColor}" />
    <SolidColorBrush x:Key="Brush.SurfaceSecondaryBackground" Color="{x:Static SystemColors.ControlColor}" />
    <SolidColorBrush x:Key="Brush.SurfaceBorder" Color="{x:Static SystemColors.WindowTextColor}" />
    <SolidColorBrush x:Key="Brush.SurfaceAltBorder" Color="{x:Static SystemColors.WindowTextColor}" />
    <SolidColorBrush x:Key="Brush.PrimaryText" Color="{x:Static SystemColors.WindowTextColor}" />
    <SolidColorBrush x:Key="Brush.SecondaryText" Color="{x:Static SystemColors.WindowTextColor}" />
    <SolidColorBrush x:Key="Brush.MutedText" Color="{x:Static SystemColors.GrayTextColor}" />
    <SolidColorBrush x:Key="Brush.PlaceholderText" Color="{x:Static SystemColors.GrayTextColor}" />
    <SolidColorBrush x:Key="Brush.CardBackground" Color="{x:Static SystemColors.WindowColor}" />
    <SolidColorBrush x:Key="Brush.CardBorder" Color="{x:Static SystemColors.WindowTextColor}" />
    <SolidColorBrush x:Key="Brush.PrimaryButtonBackground" Color="{x:Static SystemColors.HighlightColor}" />
    <SolidColorBrush x:Key="Brush.PrimaryButtonForeground" Color="{x:Static SystemColors.HighlightTextColor}" />
    <SolidColorBrush x:Key="Brush.HudBackground" Color="{x:Static SystemColors.WindowColor}" />
    <SolidColorBrush x:Key="Brush.HudCardBackground" Color="{x:Static SystemColors.WindowColor}" />
    <SolidColorBrush x:Key="Brush.HudCardBorder" Color="{x:Static SystemColors.WindowTextColor}" />
    <SolidColorBrush x:Key="Brush.HudTextPrimary" Color="{x:Static SystemColors.WindowTextColor}" />
    <SolidColorBrush x:Key="Brush.HudTextSecondary" Color="{x:Static SystemColors.GrayTextColor}" />
    <SolidColorBrush x:Key="Brush.HudProgress" Color="{x:Static SystemColors.HighlightColor}" />
    <SolidColorBrush x:Key="Brush.HudProgressBackground" Color="{x:Static SystemColors.WindowTextColor}" />
</ResourceDictionary>
